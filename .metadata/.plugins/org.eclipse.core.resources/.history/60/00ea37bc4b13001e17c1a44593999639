package constructor;

public class Rectangle {
	int length;
	int breadth;
	
	Rectangle(int length, int breadth){
		this.length = length;
		this.breadth = breadth;
	}
	
	void area() {
		int area = length*breadth;
	}
	
	public static void main(String[] args) {
		Rectangle r = new Rectangle();
		
		r.area();
	}
}
