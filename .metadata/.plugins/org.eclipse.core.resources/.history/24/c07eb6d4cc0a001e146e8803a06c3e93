
public class table {
	public static void main(String[] args) {
		System.out.printf("--------------------------------%n");
		System.out.printf("   Student Detail         %n");
  
		System.out.printf("--------------------------------%n");
		System.out.printf("| %-4s | %-8s | %3s | %4s | %10s |%n", "SN", "Name","Age","Phone","City");
		System.out.printf("--------------------------------%n");
		
		System.out.printf("| %-4s | %-8s | %010d |%n", "1.", "Ram",98561398);
		System.out.printf("| %-4s | %-8s | %010d |%n", "2.", "Sita",9033242332);
		System.out.printf("| %-4s | %-8s | %010d |%n", "3.", "Hari",);
		System.out.printf("| %-4s | %-8s | %010d |%n", "4.", "Gita",32);
		System.out.printf("| %-4s | %-8s | %010d |%n", "5.", "Shyam",16);
	

		System.out.printf("--------------------------------%n");
	}
}
