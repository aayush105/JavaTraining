package constructor;

public class User {

	String userName;
	String password;
	
	// ------------ Default Constructor --------------
//	User(){
//		userName = "test";
//		password = "123";
//	}
//	
	// ----------- parameterized constructor -----------
	
	User(String un, String psw){
		userName = un;
		password = psw;
	}
	
	void printUser() {
		System.out.println("Username : "+userName);
		System.out.println("Password : " +password);
	}
	
	public static void main(String[] args) {
//		User u = new User();
		User u = new User("Aayush","777");
		u.printUser();
	}
}
