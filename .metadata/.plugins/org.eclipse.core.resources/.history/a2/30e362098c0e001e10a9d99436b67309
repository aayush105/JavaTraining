package controlstatements.looping;

public class ForTest2 {
	
	public static void main(String[] args) {
		int so= 0;
		int se = 0;
//		for(int i=1 ; i<=100; i++) {
//			s= s+i;
//		}
//		System.out.println("Total : "+s);
		
		for ( int i = 1; i<=100; i++) {
			if(i%2==0) {
				se = se + i;
			} else {
				so = so+i;
			}
		}
	}
}
