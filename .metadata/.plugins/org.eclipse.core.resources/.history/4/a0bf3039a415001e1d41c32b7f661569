package objectwitharray;

public class Test1 {

	public static void main(String[] args) {
		Book[] books = new Book[5];

		// Create and set data for each book
		for(int i = 0;i<books.length;i++) {
			Book b= new Book();
			b.setId(1);
			b.setName("Book 1");
			b.setAuthor("Author 1");
			b.setPrice(100);
			books[i]=b;
		}
		// Print data for each book
		Test t = new Test();
		for (int i = 0; i < books.length; i++) {
			t.printBook(books[i]);
			System.out.println("------------------------");
		}
		}
}
