package datatype;

public class VariableTest {
	
	/*
	 * --------------- Variable --------------------
	 * # use to store value.
	 * # value stored for processing.
	 * 
	 * syntax :
	 *  data_type variable;
	 *  
	 */
	
	public static void main(String[] args) {
		int a = 500;
		int b = 900;
		int c = a + b;
		System.out.println("Total : " + c); // "+" concatenation : information adding
	}
}
