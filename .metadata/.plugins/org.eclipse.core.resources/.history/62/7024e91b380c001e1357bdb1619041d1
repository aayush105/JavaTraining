package datatype;

public class WrapperTest {
	
	/* --------------- Wrapper Class -------------
	 * # every primitive data type has their respective class is known as wrapper class.
	 * 
	 * Primitive Type		Wrapper class
	 * ---------------		---------------
	 * 	byte				Byte
	 *  short				Short
	 *  int					Interger
	 *  long				Long
	 *  float				Float
	 *  chat 				Character
	 *  boolean 			Boolean
	 *  
	 * */
	
	public static void main(String[] args) {
		
		// "k" is variable only
		int k = 5000;
		
		// "p" is variable and object 
		Integer p = 9000;
	}
	
}
