package oops;

public class Book {

	// properties/instance variable
	int id;
	String name;
	String author;
	int price;
	
	// methods/instance method
	void printBookInfo() {
		System.out.println("Id : "+id);
		System.out.println("Name : "+name);
		System.out.println("Author : "+author);
		System.out.println("Price : "+price);
	}
	
	
}
