package JavaCollection;

import java.util.ArrayList;
import java.util.List;

public class ListTest {

	public static void main(String[] args) {
		List<String> list = new ArrayList<>();
		list.add("hari");
		list.add("sita");
		list.add("ram");
		
		list.remove(2);
		
		for(String s : list) {
			System.out.println(s);
		}
	}
}
