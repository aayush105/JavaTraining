
public class table {
    public static void main(String[] args) {
        System.out.println("----------------------------------------");
        System.out.println("| SN | Name | Age | Phone      | City |");
        System.out.println("----------------------------------------");

        // Print table rows using a for loop
        for (int i = 1; i <= 5; i++) {
            System.out.println("| " + i + ".  | Ram  | 18  | 98986785211 | KTM  |");
            System.out.println("----------------------------------------");
        }
    }
}