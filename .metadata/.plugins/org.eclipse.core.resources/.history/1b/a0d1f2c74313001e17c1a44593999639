package constructor;

public class User {

	String userName;
	String password;
	
	// ------------ Default Constructor --------------
	User(){
		userName = "test";
	}
	
	void printUser() {
		System.out.println("Username : "+userName);
		System.out.println("Password : " +password);
	}
	
	public static void main(String[] args) {
		User u = new User();
		u.printUser();
	}
}
