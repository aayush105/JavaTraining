package constructor;

public class Average {

	int a;
	int b;
	int c;
	
	Average(){
	}
	Average(int a,int b, int c){
		this.a = a;
		this.b = b;
		this.c = c;
		
	}
	
	void calulate() {
		int avg = (a+b+c)/3;
		System.out.println("Average of three numbers is : "+avg);
	}
	
	public static void main(String[] args) {
		Average a = new Average(4,5,6);
		a.calulate();
	}
}
